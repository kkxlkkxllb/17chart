mixin panel(item,i)
	.col-md-6
		.panel.panel-primary
			.panel-heading
				h3.panel-title= "" + (i + 1) + "„ÄÅ" + item.name
			.panel-body
				a(href="#chart/#{item.id}")
					img(src="/assets/images/c#{item.id}.png")
.container
	#chart_categories
		each item,i in collection
			+panel(item,i)
			if i%2 === 1
				.clearfix
		.clearfix

